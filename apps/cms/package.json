{
    "name": "@org/cms",
    "private": true,
    "scripts": {
        "dev": "npm run generate_schemas && keystone dev",
        "build": "keystone build",
        "start": "keystone start",
        "check": "npx tsc --project tsconfig.json --noEmit",
        "test": "npx jest",
        "lint": "npx eslint --fix --ignore-path ../../.eslintignore --config .eslintrc.js '**/*.{ts,js,tsx,jsx}'",
        "format": "npx prettier --ignore-path ../../.prettierignore --write *.json",
        "generate_schemas": "keystone postinstall --fix"
    },
    "license": "MIT",
    "dependencies": {
        "@keystone-6/core": "^2.2.0",
        "lodash.capitalize": "^4.2.1",
        "lodash.kebabcase": "^4.1.1",
        "validator": "^13.7.0"
    },
    "devDependencies": {
        "@org/eslint-config-next": "*",
        "@org/jest-config-typescript": "*",
        "@org/typescript-config-base": "*",
        "@types/deepmerge": "^2.2.0",
        "@types/lodash.capitalize": "^4.2.7",
        "@types/lodash.kebabcase": "^4.1.7",
        "@types/validator": "^13.7.6"
    }
}
